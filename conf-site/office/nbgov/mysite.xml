<?xml version="1.0" encoding="UTF-8" ?>
<config>
  <httpCustom config-class="com.mymock.nutch.HttpCustom">
    <headers config-class="java.util.ArrayList">
      <header config-class="com.mymock.nutch.NameValue">
        <name>User-Agent</name>
        <value>Mozilla/5.0 (X11; Linux x86_64; rv:45.0) Gecko/20100101 Firefox/45.0</value>
      </header>
      <header config-class="com.mymock.nutch.NameValue">
        <name>Accept</name>
        <value>application/xml, text/xml, */*; q=0.01</value>
      </header>
      <header config-class="com.mymock.nutch.NameValue">
        <name>Accept-Language</name>
        <value>en-US,en;q=0.5</value>
      </header>
      <header config-class="com.mymock.nutch.NameValue">
        <name>Accept-Encoding</name>
        <value>gzip, deflate</value>
      </header>
      <header config-class="com.mymock.nutch.NameValue">
        <name>Content-type</name>
        <value>application/x-www-form-urlencoded; charset=UTF-8</value>
      </header>
      <header config-class="com.mymock.nutch.NameValue">
        <name>X-Requested-With</name>
        <value>XMLHttpRequest</value>
      </header>
    </headers>
  </httpCustom>
</config>
